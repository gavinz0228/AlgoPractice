class Solution:
    def countBits(self, num: int) -> List[int]:
        
        dp = [0]
        for i in range(1, num+1):
            log = math.log(i, 2)
            if log == math.floor(log):
                dp.append(1)
            else:
                if i % 2 == 0:
                    dp.append(dp[i - 1])
                else:
                    dp.append(dp[i - 1] + 1)
        return dp
